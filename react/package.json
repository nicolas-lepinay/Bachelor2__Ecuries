{
	"name": "facit",
	"version": "2.2.3",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"audit": "yarn audit --groups dependencies",
		"outdated": "yarn outdated",
		"serve": "serve -s build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint .",
		"lint:fix": "eslint --fix --ext .js --ext .jsx ./src",
		"lint:check-scss": "stylelint-config-prettier-scss-check",
		"lint:scss": "stylelint **/*.scss",
		"lint:fix-scss": "stylelint --fix **/*.scss",
		"icon": "svgr SvgIcons -d src/components/icon/svg-icons",
		"storybook": "start-storybook -p 6006",
		"storybook-withoutCache": "start-storybook -p 6006 --no-manager-cache",
		"build-storybook": "build-storybook"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 4 version",
			">0.2%"
		]
	},
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.36",
