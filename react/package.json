		"webpack": "5"
	},
	"peerDependencies": {
		"trim-newlines": "^4.0.2"
	},
	"resolutions": {
		"glob-parent": ">=5.1.2",
		"nth-check": ">=2.0.1",
		"trim": "^1.0.1"
	}
