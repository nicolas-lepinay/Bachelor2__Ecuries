		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.0.1",
		"@testing-library/user-event": "^14.1.0",
		"apexcharts": "^3.35.0",
		"axios": "^0.27.1",
		"classnames": "^2.3.1",
		"convert-seconds": "^1.0.1",
		"date-fns": "^2.28.0",
		"dayjs": "^1.11.0",
		"formik": "^2.2.9",
